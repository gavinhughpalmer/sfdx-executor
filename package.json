{
  "name": "sfdx-executor",
  "description": "This plugin is to execure series of sfdx commands simplifiying tasks that require a number of commands to be executed",
  "version": "1.0.0",
  "author": "Gavin Palmer",
  "bugs": "https://github.com/gavinhughpalmer/sfdx-executor/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.3",
    "@salesforce/command": "3.0.3",
    "@salesforce/core": "2.12.2",
    "tslib": "2.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/plugin-help": "3.2.0",
    "@oclif/test": "1.2.7",
    "@salesforce/dev-config": "1.6.0",
    "@salesforce/ts-sinon": "1.2.3",
    "@types/chai": "4.2.12",
    "@types/mocha": "5.2.7",
    "@types/node": "10.17.35",
    "chai": "4.2.0",
    "globby": "11.0.1",
    "mocha": "5.2.0",
    "nyc": "15.1.0",
    "sfdx-cli": "7.74.1",
    "ts-node": "9.0.0",
    "tslint": "6.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/gavinhughpalmer/sfdx-executor",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "gavinhughpalmer/sfdx-executor",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "fix": "tslint -p test -t stylish --fix"
  }
}
